{
  "name": "@bblabs/mindfulness",
  "version": "1.4.0",
  "description": "Simple logging and metrics interfaces",
  "repository": "https://github.com/bombbomb/mindfulness",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest",
    "test-coverage": "jest --coverage",
    "test-watch": "jest --watch",
    "build": "tsc",
    "build-watch": "tsc --watch",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags",
    "eslint": "eslint src/**/*.ts",
    "eslint-ci": "eslint --quiet src/**/*.ts"
  },
  "keywords": [
    "logging",
    "metrics"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "0.0.31",
    "@types/jest": "^22.0.1",
    "@types/nock": "^9.3.0",
    "@types/request-promise-native": "^1.0.15",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.4",
    "jest": "^22.4.4",
    "jest-mock-console": "github:PiereDome/jest-mock-console#6a5e831aa7a2b8a53b1af5fd2d230b27299a0e66",
    "mute": "^2.0.6",
    "nock": "^10.0.4",
    "sinon": "^4.1.5",
    "ts-jest": "^22.4.6",
    "ts-node": "^7.0.1",
    "typescript": "^2.9.2",
    "typescript-eslint-parser": "^12.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "(/test/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "debug": "^4.1.0"
  }
}
